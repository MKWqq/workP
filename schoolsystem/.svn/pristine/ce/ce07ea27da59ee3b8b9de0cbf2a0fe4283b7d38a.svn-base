@import '../common';
.coursePrevious{/*1646内容1582*/
  width:100%;padding:20/16rem 32/1646*100%;.box-sizing();
  .g-timeHeader{
    button{}
    .g-gobackChart{}
    .blueButton{}
  }
}
.g-classesTimeSetSection{/*1582*/
  width:100%;.marginTop(30);
  &>div{float:left;}
  .g-sectionL{/*230*/
    .width(230,1582);border:1px solid @borderColor;.border-radius(6/16rem);.box-sizing();
    .gL-header{width:100%;border-bottom:1px solid @borderColor;.box-sizing();
      h2{.width(206,230);margin:20/16rem 18/230*100% 14/16rem;font-size:1rem;color:@HColor;display:inline-block;}
      .fuzzyInput{.width(206,230);margin:0 18/230*100% 20/16rem;}
    }
    .gL-section{/*230*/
      /*tree*/
      .width(206,230);.height(712);margin:20/16rem 0 0 12/230*100%;padding-bottom:14/16rem;.box-sizing();overflow-x:hidden;overflow-y:auto;
      &>ul{width:100%;
        li:not(:last-of-type){.marginBottom(15);}
        &>li{width:100%;.fontSize(14);.height(28);text-align:center;.border-radius(14/16rem);
          &:hover{cursor:pointer;}
        }
        li.activeLi{color:#fff;background:@backgroundBlue;}
        li.NormalLi{color:@normalColor;background:none;}
      }
    }
  }
  .g-sectionC{/*162*/
    .width(162,1582);.marginLeft(20,1582);border:1px solid @borderColor;.border-radius(6/16rem);.box-sizing();
    .gL-header{width:100%;border-bottom:1px solid @borderColor;.box-sizing();
      h2{.width(144,162);margin:20/16rem 18/162*100% 14/16rem;font-size:1rem;color:@HColor;display:inline-block;}
      .fuzzyInput{.width(144,162);margin:0 18/162*100% 20/16rem;}
    }
    .gL-section{
      /*tree*/
      .width(376,412);.height(712);margin:20/16rem 0 0 18/412*100%;padding-bottom:14/16rem;.box-sizing();overflow-x:hidden;overflow-y:auto;

    }
  }
  .g-sectionR{/*1142*/
    .width(1142,1582);.marginLeft(20,1582);
    &>header{width:100%;border:1px solid @elementUiBorder;.box-sizing();
      h2{width:100%;font-size:1rem;color:@HColor;.height(50);.marginLeft(20,1142);}
    }
    .g-timeSettingTable{
      span{display:inline-block;.height(75);width:100%;.border-radius(10/16rem);}
      .NotCourse{background:@intervalColor;color:@normalColor;}
      .SetNotCourse{background:@green;color:#fff;}
    }
  }
}




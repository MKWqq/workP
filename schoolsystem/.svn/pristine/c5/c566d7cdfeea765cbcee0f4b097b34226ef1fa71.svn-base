@import '../common';
.g-ManualScheduce{/*1646内容1582*/
  width:100%;padding:20/16rem 32/1646*100%;.box-sizing();
  .g-timeHeader{width:100%;
    .g-buttonGroup{width:100%;
      button{}
      .g-gobackChart{}
      .blueButton{}
    }
    .g-selectGroup{width:100%;.marginTop(30);
      .el-form-item{float:left;margin-bottom:0;.marginRight(30,1582);}
    }
  }
}
.g-classesTimeSetSection{/*1582*/
  width:100%;.marginTop(30);
  &>div{float:left;}
  .g-sectionL{/*258,用的是比例*/
    .width(258,1582);border:1px solid @borderColor;.border-radius(6/16rem);.box-sizing();
    .gL-header{width:100%;border-bottom:1px solid @borderColor;.box-sizing();
      h2{.width(376,412);margin:20/16rem 18/412*100% 14/16rem;font-size:1rem;color:@HColor;display:inline-block;}
      .fuzzyInput{.width(376,412);margin:0 18/412*100% 20/16rem;}
    }
    .gL-section{
      /*tree*/
      .width(376,412);.height(712);margin:20/16rem 0 0 18/412*100%;padding-bottom:14/16rem;.box-sizing();overflow-x:hidden;overflow-y:auto;
      .g-contactSearch{width:100%;.height(40-5);background:#dadada;position:relative;
        img{.widthRem(15);position:absolute;top:13/16rem;left:28/16rem;}
        input{text-indent:37/16rem;.width(268,304);.marginLeft(18,304);border:1px solid @borderColor;.box-sizing();.border-radius(28/2/16rem);font-size:14/16rem;.height(28);color:#282828;background:#fff;
          &:focus{outline:none;}
        }
      }
      ul.g-contactCon{width:100%;.NotLineheight(580-56-40);overflow-x:hidden;overflow-y:auto;
        li.g-contactRow{width:100%;.NotLineheight(64);
          .g-contactImg{.width(44,304);.height(64);.marginLeft(22,304);float:left;
            img{.widthRem(44);}
          }
          .g-contactMsg{float:left;.width(216,304);height:100%;.marginLeft(10,304);border-bottom:1px solid @borderColor;.box-sizing();
            p{.width(200,216);.marginLeft(16,216);.NotLineheight(25);font-size:15/16rem;}
            .s-contactText{.marginTop(14);color:#979797;}
            .s-contactPhone{color:#3c3c3c;}
          }
        }
      }
    }
  }
  .g-sectionR{/*640，用的是比例*/
    .width(640,1582);.marginLeft(20,1582);
    &>header{width:100%;border:1px solid @elementUiBorder;.box-sizing();
      h2{width:100%;font-size:1rem;color:@HColor;.height(50);.marginLeft(20,1142);}
    }
    .g-timeSettingTable{
      span{display:inline-block;.height(40);width:100%;.border-radius(10/16rem);}
      .NotCourse{background:@intervalColor;color:@normalColor;}
      .SetNotCourse{background:@green;color:#fff;}
    }
  }

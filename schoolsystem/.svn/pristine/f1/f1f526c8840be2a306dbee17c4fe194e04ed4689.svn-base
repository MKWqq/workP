.alertTitle {
  cursor: pointer;
}

.alertTitle:hover {
  text-decoration: underline;
}

/*分界线*/
.d_line{
  height:1px;
  background-color: #d2d2d2;
}
/*wqq*/
/*模糊查询css*/
.g-fuzzyInput{width:18.75rem;}
.g-fuzzyInput input{height:28px;
  -webkit-border-radius: 14px;
  -moz-border-radius: 14px;
  border-radius: 14px;}
.g-fuzzyInput .el-input__icon{background:#13b5b1;
  border-bottom-right-radius:14px;border-top-right-radius:14px;}
.g-fuzzyInput .el-icon-search:before{color:#fff;}


/*table固定栏*/
.g-fixedColumn .el-table__fixed-header-wrapper thead div{background:none;color:#fff;}






